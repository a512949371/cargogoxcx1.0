/* pages/personal/personal.wxss */
.personal-body{
  display: flex;
  align-items: center;
  flex-direction: column;
  padding-top: 50rpx;
  
}

.personal-topicon{
  width: 140rpx;
  height: 140rpx;
  margin-bottom: 37rpx;
  border-radius: 75rpx;
}

.personal-name{
  font-size: 45rpx;
  color: #000;
}

.personal-nav{
  width: 100%;
  display: -webkit-flex;
  display: -webkit-box;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  margin-top: 30rpx;
}

.personal-nav-item{
  width: 33%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 63rpx;
}

.personal-nav-item image{
  width: 80rpx;
  height: 52rpx;
  margin-bottom: 0rpx;
}

.personal-nav-item text{
  font-size: 25rpx;
  margin-top: 20rpx;
}