/* pages/footprint/footprint.wxss */
.flex-box{
  display: flex;
  align-items: center;
}
.flex-box-btween{
  display: flex;
  align-items: center;
  justify-content: space-between
}
.flex-box-center{
  display: flex;
  align-items: center;
  justify-content: center
}
.flex-box-end{
  display: flex;
  align-items: center;
  justify-content: flex-end
}
.pt5{
  padding-top: 10rpx;
}
.pt10{
  padding-top: 20rpx;
}
.pt20{
  padding-top: 40rpx;
}
.pt40{
  padding-top: 80rpx;
}
.pt60{
  padding-top: 120rpx;
}
.pt100{
  padding-top: 200rpx;
}
.pl5{
  padding-left: 10rpx;
}
.pl15{
  padding-left: 30rpx;
}
.pr5{
  padding-right: 10rpx;
}
.pr15{
  padding-right: 30rpx;
}
.pr30{
  padding-right: 60rpx;
}
.ml10{
  margin-left: 10rpx;
}
/* 列表 */
.list-box{
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.list-box .list{
  padding: 20rpx 0;
  border-bottom: 1px solid #e5e5e5
}
.list-box .list .left{
  width: 279rpx;
  height: 156rpx;
}
.list-box .list .left image{
  width: 279rpx;
  height: 156rpx;
}
.list-box .list .right{
  flex: 1
}
.list-box .list .right .name{
  font-size: 30rpx;
  color: #141414
}
.list-box .list .right .desc{
  font-size: 24rpx;
  color: #646464
}
.list-box .list .right .price-box .price-left{
  font-size: 24rpx;
  color: #FF4D30
}
.list-box .list .right .price-box .price{
  font-size: 30rpx;
  font-weight: bold
}
.list-box .list .right .price-box .price-right{
  font-size: 24rpx;
  color: #969696;
}
.list-box .list .right .tip-box{
  flex-wrap: wrap
}
.list-box .list .right .tip-box .tip{
  background-color: #f0f0f0;
  border-radius: 4rpx;
  padding: 4rpx 10rpx;
  font-size: 20rpx;
  color: #969696
}
.fixed-del{
  position: fixed;
  right: 20rpx;
  bottom: 60rpx;
  z-index: 99
}
.fixed-del image{
  width: 119rpx;
  height: 119rpx;
}

.fixed{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999
}
.fixed .shaw{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10000;
  background-color: rgba(0, 0, 0, 0.5)
}
.fixed .content{
  width:100px;
  height:100px;
  position: fixed;
  top:calc(50% - 50px);
  left:calc(50% - 50px);
  z-index: 10001;
  border-radius: 8px;
  background-color: rgba(0, 0, 0, 0.5)
}
.fixed .text-box{
  width:100px;
  height:100px;
  position: fixed;
  top:calc(50% - 30px);
  left:calc(50% - 50px);
  z-index: 10002;
  text-align: center;
}
.fixed .text-box image{
  width: 40px;
  height: 40px;
  display: inline-block;
}
.fixed .text-box .desc{
  font-size: 28rpx;
  color: #FFF;
}