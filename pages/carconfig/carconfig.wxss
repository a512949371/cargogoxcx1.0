/* pages/carconfig/carconfig.wxss */
page{
  width: 100%;
  height: 100%;
}
.flex-box{
  display: flex;
  align-items: center;
}
.flex-box-btween{
  display: flex;
  align-items: center;
  justify-content: space-between
}
.flex-box-center{
  display: flex;
  align-items: center;
  justify-content: center
}
.flex-box-end{
  display: flex;
  align-items: center;
  justify-content: flex-end
}
.pt5{
  padding-top: 10rpx;
}
.pt10{
  padding-top: 20rpx;
}
.pt20{
  padding-top: 40rpx;
}
.pt40{
  padding-top: 80rpx;
}
.pt60{
  padding-top: 120rpx;
}
.pt100{
  padding-top: 200rpx;
}
.pl5{
  padding-left: 10rpx;
}
.pl15{
  padding-left: 30rpx;
}
.pr5{
  padding-right: 10rpx;
}
.pr15{
  padding-right: 30rpx;
}
.pr30{
  padding-right: 60rpx;
}
.mt20{
  margin-top: 40rpx;
}
.cartit{
  padding: 20rpx;
  font-size: 36rpx;
  color: #141414;
}
.carconfig-box{
  height: 100%;
}
.carconfig-list-tit{
  font-size: 26rpx;
  color: #141414;
  padding: 20rpx;
  background-color: #F5F5F5
}
.carconfig-list{
  padding: 0 20rpx;
}
.carconfig-list .box{
  padding: 20rpx 0;
  border-bottom: 1px solid #e5e5e5;
}
.carconfig-list .box .left{
  font-size: 26rpx;
  color: #646464;
}
.carconfig-list .box .right{
  font-size: 26rpx;
  color: #141414;
}
.fixed-home{
  position: fixed;
  bottom: 10rpx;
  right: 30rpx;
  z-index: 99
}
.fixed-home .home image{
  width: 120rpx;
  height: 120rpx;
}
.fixed-home .more image{
  width: 120rpx;
  height: 120rpx;
  position: relative;
  z-index: 100
}
.fixed-par{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
}
.fixed-par .shaw{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.5)
}
.fixed-par .content-box{
  background-color: #FFF;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 101;
  max-height: 300px;
  padding:20rpx;
  flex-wrap: wrap;
  overflow-y: scroll;
}
.fixed-par .content-box .list-box{
  width: 33%;
  box-sizing: border-box;
  padding:15rpx 20rpx;
}
.fixed-par .content-box .list-box .list{
  border-radius: 30rpx;
  padding: 15rpx 0; 
  font-size: 24rpx;
  text-align: center;
  color: #969696;
  background-color: #f5f5f5;
}

.fixed{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999
}
.fixed .shaw{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10000;
  background-color: rgba(0, 0, 0, 0.5)
}
.fixed .content{
  width:100px;
  height:100px;
  position: fixed;
  top:calc(50% - 50px);
  left:calc(50% - 50px);
  z-index: 10001;
  border-radius: 8px;
  background-color: rgba(0, 0, 0, 0.5)
}
.fixed .text-box{
  width:100px;
  height:100px;
  position: fixed;
  top:calc(50% - 30px);
  left:calc(50% - 50px);
  z-index: 10002;
  text-align: center;
}
.fixed .text-box image{
  width: 40px;
  height: 40px;
  display: inline-block;
}
.fixed .text-box .desc{
  font-size: 28rpx;
  color: #FFF;
}